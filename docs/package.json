{
	"name": "@svecodocs/docs",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "pnpm \"/dev:/\"",
		"dev:content": "velite dev --watch",
		"dev:generate": " pnpm build:release",
		"dev:svelte": "vite dev",
		"build": "velite && node ./scripts/update-velite-output.js && pnpm build:search && pnpm build:release && pnpm build:sitemap && vite build",
		"build:sitemap": "node ./scripts/build-sitemap.js",
		"build:release": "node ./scripts/build-release-data.js",
		"build:search": "node ./scripts/build-search-data.js",
		"preview": "vite preview",
		"check": "velite && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "pnpm build:content && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"devDependencies": {
		"@svecodocs/kit": "latest",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/jsdom": "^27.0.0",
		"@types/node": "^24.7.1",
		"@types/unist": "^3.0.3",
		"consola": "^3.4.2",
		"jsdom": "^27.2.0",
		"mdsx": "^0.0.6",
		"phosphor-svelte": "^3.0.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"rehype-parse": "^9.0.1",
		"rehype-remark": "^10.0.1",
		"rehype-remove-comments": "^6.1.1",
		"remark-gfm": "^4.0.1",
		"remark-stringify": "^11.0.0",
		"svelte": "^5.43.12",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"unified": "^11.0.5",
		"unist-util-visit-parents": "^6.0.1",
		"velite": "^0.2.4",
		"vite": "^7.2.4"
	},
	"dependencies": {
		"@ethercorps/sveltekit-og": "^4.2.0",
		"octokit": "^5.0.5"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
