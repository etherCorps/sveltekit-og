{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "sveltekit-og",
  "main": ".svelte-kit/cloudflare/_worker.js",
  "routes": [
    {
      "pattern": "shop.example.com",
      "custom_domain": true
    }
  ],
  "workers_dev": false,
  "compatibility_date": "2025-11-20",
  "build": {
    "command": "pnpm run build"
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    },
    "traces": {
      "enabled": true,
      "persist": true,
      "head_sampling_rate": 1
    }
  },
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": ".svelte-kit/cloudflare"
  },
  "rules": [
    {
      "type": "CompiledWasm",
      "globs":[ "**/*.wasm", "**/*.wasm?module"],
      "fallthrough": true
    },
    {
      "type": "Data",
      "globs": ["**/*.ttf"],
      "fallthrough": true
    }
  ]
}